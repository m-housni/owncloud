{
  "server": "http://owncloud/",
  "theme": "http://owncloud/themes/owncloud/theme.json",
  "openIdConnect": {
    "metadata_url": "https://idp:9130/.well-known/openid-configuration",
    "authority": "https://idp:9130",
    "client_id": "web",
    "response_type": "code",
    "scope": "openid profile email"
  },
  "options": {
    "topCenterNotifications": true,
    "displayResourcesLazy": false,
    "sidebar": {
      "shares": {
        "showAllOnLoad": true
      }
    },
    "routing": {
      "idBased": false
    }
  },
  "apps": [
    "files",
    "draw-io",
    "text-editor",
    "preview",
    "pdf-viewer",
    "search"
  ]
}
